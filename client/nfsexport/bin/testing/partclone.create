#!/bin/bash
OGAPI=/opt/opengnsys/lib/engine/bin/
source $OGAPI/System.lib 
source $OGAPI/Disk.lib 
source $OGAPI/FileSystem.lib 
while getopts "cFs:" flags
do
   case "$flags" in
   "s")
   	PART=`echo $OPTARG` 
   	FS=$(ogGetFsType `ogDevToDisk $PART` | awk '{print tolower($1)}')
	#PARAM1="partclone.$FS --clone --force --source $PART"	  
	PARAM1="partclone.$FS $*"
	echo $PARAM1	
   ;;
   esac  
done	




